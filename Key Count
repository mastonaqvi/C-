#include<iostream>
using namespace std;

int main()
{
	int num1[5];

	for (int i = 0; i <= 4; i++)
	{
		cout << "enter number" << i << endl;
		cin >> num1[i];
	}
	cout << endl;
	for (int i = 0; i <= 4; i++)
	{
		cout << num1[i] << ",";
	}
	cout << endl;
	int r = 0;
	cout << "key" << endl;
	cin >> r;
	int count = 0;
	for (int i = 0; i <= 4; i++)
	{

		if (r == num1[i])
		{
			count++;
		}
	
	}
	cout << "key count = " << count;



	return 0;
}
