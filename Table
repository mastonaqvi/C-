#include <iostream> 
using namespace std; 
int main() 
{
    int a, i = 0; 

    cout << " Print the multiplication table of a number up to 10:\n"; 
    cout << "--------------------------------------------------------\n";
    cout << " Input a number: ";
    cin >> a; 

    for (i = 1; i <= 10; i++) // Start of a 'For' loop that iterates from 1 to 10
    {
        cout << a << " x " << i << " = " << a * i << "\n"; 
    }

    return 0; 
}
