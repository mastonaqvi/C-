#include <iostream>
using namespace std;

int main() {
    int size;
    cout << "Enter the size of the Array: ";
    cin >> size;

    int array[size];

    cout << "Enter Numbers: ";
    for (int n = 0; n < size; n++) {
        cin >> array[n];
    }
    cout << endl;
    cout<<endl;
    cout<<"Duplicate Numbers:" ;
    cout << "Duplicate Numbers: ";
    for(int i=0; i<size; i++)
    for (int i = 0; i < size; i++) {
        for (int j = i + 1; j < size; j++) {
            if (array[i] == array[j]) {
                cout << array[i] << " ";
            }
        }
    }
    cout << endl;

    return 0;
    
}
